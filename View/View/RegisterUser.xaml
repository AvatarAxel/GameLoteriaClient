<Window x:Class="View.RegisterUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="RegisterUser" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Margin="126,76,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Margin="126,126,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="txtPassword1" HorizontalAlignment="Left" Margin="126,157,0,0" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="txtPassword2" HorizontalAlignment="Left" Margin="126,199,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Acceptar" HorizontalAlignment="Left" Margin="344,157,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="Birtheday" HorizontalAlignment="Left" Margin="126,254,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
